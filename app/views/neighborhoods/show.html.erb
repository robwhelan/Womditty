		<div class="col-sm-6 col-md-4">
			<iframe src=<%= @neighborhood.education_level %> width="340" height="630" scrolling="no" frameborder="0" style="border:none;"></iframe>
		</div>
		<div class="col-sm-6 col-md-4">
			<iframe src=<%= @neighborhood.drive_time %> width="340" height="630" scrolling="no" frameborder="0" style="border:none;"></iframe>
		</div>
	</div>
	
<!-- love -->
