<h1>Listing inquiries</h1>

<table>
  <tr>
    <th>Rank</th>
    <th>Command</th>
    <th>Number of people</th>
    <th>Number of bedrooms</th>
    <th>Move date</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Phone number</th>
    <th>User</th>
    <th>Vendor</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inquiries.each do |inquiry| %>
  <tr>
    <td><%= inquiry.rank %></td>
    <td><%= inquiry.command %></td>
    <td><%= inquiry.number_of_people %></td>
    <td><%= inquiry.number_of_bedrooms %></td>
    <td><%= inquiry.move_date %></td>
    <td><%= inquiry.first_name %></td>
    <td><%= inquiry.last_name %></td>
    <td><%= inquiry.email %></td>
    <td><%= inquiry.phone_number %></td>
    <td><%= inquiry.user %></td>
    <td><%= inquiry.vendor %></td>
    <td><%= link_to 'Show', inquiry %></td>
    <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
    <td><%= link_to 'Destroy', inquiry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Inquiry', new_inquiry_path %>
