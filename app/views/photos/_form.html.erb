<%= form_for @photo, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
	<%= f.file_field :image %>

    <%= f.label :image_url, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :image_url, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :place_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :place_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :neighborhood_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :neighborhood_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :user_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :user_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :city_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :city_id, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                photos_path, :class => 'btn' %>
  </div>
<% end %>
