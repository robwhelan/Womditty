<%= form_for @user, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :firstname, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :firstname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :lastname, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :lastname, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :member_rank, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :member_rank, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :number_of_invites, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :number_of_invites, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :has_kids, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :has_kids, :class => 'check_box' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :invited_by_user_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :invited_by_user_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :role, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :role, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :profile_image_url, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :profile_image_url, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :city_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :city_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :career_specialty_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :career_specialty_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :duty_station_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :duty_station_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :military_branch_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :military_branch_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :neighborhood_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :neighborhood_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :email, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :email, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :encrypted_password, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :encrypted_password, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :reset_password_token, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :reset_password_token, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :reset_password_sent_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.datetime_select :reset_password_sent_at, :class => 'datetime_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :remember_created_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.datetime_select :remember_created_at, :class => 'datetime_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :current_sign_in_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.datetime_select :current_sign_in_at, :class => 'datetime_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :last_sign_in_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.datetime_select :last_sign_in_at, :class => 'datetime_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :current_sign_in_ip, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :current_sign_in_ip, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :last_sign_in_ip, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :last_sign_in_ip, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                users_path, :class => 'btn' %>
  </div>
<% end %>
